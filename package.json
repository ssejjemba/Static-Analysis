  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.12.1",
    "@babel/core": "7.12.3",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-typescript": "7.12.1",
    "@typescript-eslint/eslint-plugin": "4.5.0",
    "@typescript-eslint/parser": "4.5.0",
    "eslint": "7.12.0",
    "eslint-config-prettier": "6.14.0",
    "husky": "4.3.0",
    "lint-staged": "10.4.2",
    "prettier": "2.1.2",
    "typescript": "4.0.3"
  }
